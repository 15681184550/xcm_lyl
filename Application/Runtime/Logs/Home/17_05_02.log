SQL: INSERT INTO `jia_api` (`type`,`answer`,`chapter`,`explain`,`option1`,`option2`,`option3`,`option4`,`pic`,`question`,`type_`,`create_time`) VALUES ('C1','对','安全行车、文明驾驶基础知识','雾天视线不佳，喇叭可当做一种信号和其他车辆交流，有利于安全行车。','','','','','','雾天行车多使用喇叭可引起对方注意；听到对方车辆鸣喇叭，也应鸣喇叭回应。','','2017-05-02 11:34:03') [ RunTime:0.0010s ]

