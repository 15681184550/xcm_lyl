SQL: INSERT INTO `jia_api` (`type`,`answer`,`chapter`,`explain`,`option1`,`option2`,`option3`,`option4`,`pic`,`question`,`type_`,`create_time`) VALUES ('C1','C','安全行车、文明驾驶基础知识','超车前，应选择视线良好、平直宽阔、左右均无障碍且前方路段200米范围内没有来车的道路，在保证安全的前提下方能超车。千万不要盲目超车。几个选项中，当然是选路宽车少的路段了。','A、窄路窄桥','B、交叉路口','C、路宽车少','D、弯道陡坡','','夜间行车，可选择下列哪个地段超车。','','2017-05-02 11:35:03') [ RunTime:0.0010s ]

