SQL: INSERT INTO `jia_api` (`type`,`answer`,`chapter`,`explain`,`option1`,`option2`,`option3`,`option4`,`pic`,`question`,`type_`,`create_time`) VALUES ('C1','对','道路交通安全法律、法规和规章','重点看题目中的“专用使用时间之外”，审题要仔细。在规定时间内，道路划设为专用车道的，在专用车道内，只准许规定的车辆通行，其他车辆不得进入专用车道内行驶。在规定时间之外，其他车辆可以行使。','','','','','','专用车道规定的专用使用时间之外，其他车辆可以进入专用车道行驶。','','2017-05-02 10:56:59') [ RunTime:0.0010s ]

