SQL: INSERT INTO `jia_api` (`type`,`answer`,`chapter`,`explain`,`option1`,`option2`,`option3`,`option4`,`pic`,`question`,`type_`,`create_time`) VALUES ('C1','C','安全行车、文明驾驶基础知识','下坡不允许超车，超车选择宽阔的缓上坡路段比较安全。上坡有阻力，不宜滑车，也不宜撞车，所以上坡宽阔地可以超车。','A、选择较缓的下坡路段超车','B、选择任意路段超车','C、选择宽阔的缓上坡路段超车','D、选择较长的下坡路段超车','','在山区道路超车时，应怎样超越？','','2017-05-02 11:32:03') [ RunTime:0.0010s ]

