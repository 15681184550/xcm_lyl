SQL: INSERT INTO `jia_api` (`type`,`answer`,`chapter`,`explain`,`option1`,`option2`,`option3`,`option4`,`pic`,`question`,`type_`,`create_time`) VALUES ('C1','D','安全行车、文明驾驶基础知识','车辆在平面路上行驶，灯光距离不变；在出现一般的弯道，灯光由路中移动到路侧；上坡时，灯光由高变低；出现急弯或大坑时，灯光离开路面。','A、距离不变','B、由高变低','C、离开路面','D、由路中移到路侧','','夜间行车中，前方出现弯道时，灯光照射会发生怎样的变化？','','2017-05-02 11:33:03') [ RunTime:0.0010s ]

