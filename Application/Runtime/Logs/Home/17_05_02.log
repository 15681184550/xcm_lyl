SQL: INSERT INTO `jia_api` (`type`,`answer`,`chapter`,`explain`,`option1`,`option2`,`option3`,`option4`,`pic`,`question`,`type_`,`create_time`) VALUES ('C1','对','安全行车、文明驾驶基础知识','通过弯道时，大家都知道要减速，而在山区道路，由于前方状况很难了解，因此，若要经过弯道，应当鸣号告知其他车辆你要通过弯道了，靠右行是为了避免在弯道时遇到对向来车，若不靠右行驶，很可能会与来车发生碰撞。','','','','','','车辆在通过山区道路弯道时，要做到“减速、鸣喇叭、靠右行”。','','2017-05-02 11:32:03') [ RunTime:0.0010s ]

