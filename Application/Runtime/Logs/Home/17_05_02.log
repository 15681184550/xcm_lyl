SQL: INSERT INTO `jia_api` (`type`,`answer`,`chapter`,`explain`,`option1`,`option2`,`option3`,`option4`,`pic`,`question`,`type_`,`create_time`) VALUES ('C1','错','安全行车、文明驾驶基础知识','在行驶中应该打开雾灯、尾灯、示廓灯和前照灯(近光)，不能使用远光灯。因远光灯光轴偏上，射出的光线被雾气漫反射，在车前形成白茫茫一片，如同隔着磨砂玻璃一样，反而什么都看不见。雾天行车可以靠路面车道标线以及前车的红色尾灯来行车。','','','','','','雾天行车为了提高能见度，应该开启远光灯。','','2017-05-02 11:01:04') [ RunTime:0.0010s ]

