SQL: INSERT INTO `jia_api` (`type`,`answer`,`chapter`,`explain`,`option1`,`option2`,`option3`,`option4`,`pic`,`question`,`type_`,`create_time`) VALUES ('C1','错','安全行车、文明驾驶基础知识','根据规定，在没有中心线的道路上，机动车应该靠中间行驶，留两边道路给行人和非机动车行驶。','','','','','','驾驶机动车在没有道路中心线的道路上行驶，应该在道路的左侧通行。','','2017-05-02 11:01:04') [ RunTime:0.0020s ]

