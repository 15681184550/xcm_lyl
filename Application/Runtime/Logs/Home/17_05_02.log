SQL: INSERT INTO `jia_api` (`type`,`answer`,`chapter`,`explain`,`option1`,`option2`,`option3`,`option4`,`pic`,`question`,`type_`,`create_time`) VALUES ('C1','B','安全行车、文明驾驶基础知识','安全第一，文明驾驶，礼貌让行。','A、向左占道，谨慎驶过','B、提前减速或停车避让','C、保持正常车速行驶','D、鸣喇叭催其让行','','车辆在较窄的山路上行驶时，如果靠山体的一方不让行，应怎样做？','','2017-05-02 11:32:03') [ RunTime:0.0010s ]

