SQL: INSERT INTO `jia_api` (`type`,`answer`,`chapter`,`explain`,`option1`,`option2`,`option3`,`option4`,`pic`,`question`,`type_`,`create_time`) VALUES ('C1','对','道路交通安全法律、法规和规章','《机动车驾驶证申领和使用规定》第四十二条机动车驾驶人具有下列情形之一的，车辆管理所应当注销其机动车驾驶证:(一)死亡的;(二)身体条件不适合驾驶机动车的;(三)提出注销申请的;(四)丧失民事行为能力，监护人提出注销申请的;(五)超过机动车驾驶证有效期一年以上未换证的。','','','','','','机动车驾驶证有效期超过一年以上未换证的，驾驶证将被注销。','','2017-05-02 11:22:00') [ RunTime:0.0010s ]

