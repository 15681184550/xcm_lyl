SQL: INSERT INTO `jia_api` (`type`,`answer`,`chapter`,`explain`,`option1`,`option2`,`option3`,`option4`,`pic`,`question`,`type_`,`create_time`) VALUES ('C1','错','安全行车、文明驾驶基础知识','不能开启远光灯，因浓雾会反射灯光。应开启雾灯。','','','','','','浓雾天气能见度低，开启远光灯会提高能见度。','','2017-05-02 11:33:03') [ RunTime:0.0020s ]

