SQL: INSERT INTO `jia_api` (`type`,`answer`,`chapter`,`explain`,`option1`,`option2`,`option3`,`option4`,`pic`,`question`,`type_`,`create_time`) VALUES ('C1','C','安全行车、文明驾驶基础知识','浓雾或特大雾天,能见度过低，因此要开启危险报警闪光灯和雾灯，选择安全地点停车。','A、开启前照灯，继续行驶','B、开启示廓灯、雾灯，靠右行驶','C、开启危险报警闪光灯和雾灯，选择安全地点停车','D、开启危险报警闪光灯，继续行驶','','遇有浓雾或特大雾天能见度过低，行车困难时，应怎样做？','','2017-05-02 11:33:03') [ RunTime:0.0010s ]

