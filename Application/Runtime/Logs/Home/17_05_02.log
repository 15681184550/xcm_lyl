SQL: INSERT INTO `jia_api` (`type`,`answer`,`chapter`,`explain`,`option1`,`option2`,`option3`,`option4`,`pic`,`question`,`type_`,`create_time`) VALUES ('C1','D','安全行车、文明驾驶基础知识','是双方选择怎么交会，两个车可以等到安全路段再减速交会，这样更安全。因此本题答案选D。','A、不减速','B、紧靠道路中心','C、加速','D、减速或停车让行','','在山区道路遇对向来车时，应怎样会车？','','2017-05-02 11:32:03') [ RunTime:0.0000s ]

