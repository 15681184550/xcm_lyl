SQL: INSERT INTO `jia_api` (`type`,`answer`,`chapter`,`explain`,`option1`,`option2`,`option3`,`option4`,`pic`,`question`,`type_`,`create_time`) VALUES ('C1','对','安全行车、文明驾驶基础知识','先观察与判断汽车侧后方的交通情况，然后打开转向灯；再次确认两侧道路没有车辆超越，并保持准备驶入的车道留有安全距离；平稳转向，再驶往所需车道，完成后关闭转向灯即可。','','','','','','驾驶机动车变更车道前应仔细观察，目的是判断有无变更车道的条件。','','2017-05-02 11:35:03') [ RunTime:0.0010s ]

