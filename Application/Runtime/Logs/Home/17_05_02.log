SQL: INSERT INTO `jia_api` (`type`,`answer`,`chapter`,`explain`,`option1`,`option2`,`option3`,`option4`,`pic`,`question`,`type_`,`create_time`) VALUES ('C1','对','安全行车、文明驾驶基础知识','通过没有路灯或路灯照明不良的路段（非路口）时可以开启远光灯，但是前方有车或行人时，为了不影响他人视线，应关闭远光，改用近光。','','','','','','夜间通过没有路灯或路灯照明不良时，应将近光灯转换为远光灯，但同向行驶的后车不得使用远光灯。','','2017-05-02 11:33:03') [ RunTime:0.0030s ]

