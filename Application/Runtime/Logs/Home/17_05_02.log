SQL: INSERT INTO `jia_api` (`type`,`answer`,`chapter`,`explain`,`option1`,`option2`,`option3`,`option4`,`pic`,`question`,`type_`,`create_time`) VALUES ('C1','错','安全行车、文明驾驶基础知识','安全第一,雾天严禁超车；暴雨/大雨导致刮雨器不能满足要求时，选择安全地点停车；风沙特别大时，车停道路上风处，车头背向风沙，关闭百叶窗；冰雪行车，装防滑链，用发动机制动控制车速，低速行驶，选择较安全地，靠右侧慢行，适当增大两车横向距离，且与路边保持一定距离，纵向距离为正常情况下1.5-3倍。','','','','','','风、雨、雪、雾等复杂气象条件，遇前车速度较低时，应开启前照灯，连续鸣喇叭迅速超越。','','2017-05-02 11:33:03') [ RunTime:0.0010s ]

