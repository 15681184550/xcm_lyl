SQL: INSERT INTO `jia_api` (`type`,`answer`,`chapter`,`explain`,`option1`,`option2`,`option3`,`option4`,`pic`,`question`,`type_`,`create_time`) VALUES ('C1','对','安全行车、文明驾驶基础知识','冰雪路面行驶时，制动距离比平时要大。因此要考虑到紧急状况。如果出现紧急情况，需要有足够的制动距离。','','','','','','在冰雪路面上行车，必须降低车速、加大安全距离。','','2017-05-02 11:34:03') [ RunTime:0.0010s ]

