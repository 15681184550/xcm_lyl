SQL: INSERT INTO `jia_api` (`type`,`answer`,`chapter`,`explain`,`option1`,`option2`,`option3`,`option4`,`pic`,`question`,`type_`,`create_time`) VALUES ('C1','错','安全行车、文明驾驶基础知识','发动机制动是利用发动机的牵阻作用减慢车速，档位越低牵阻越明显，制动性越强。在下长坡道路行驶，挂入低速档利用发动机的牵阻作用可以减少制动器的负担和减少制动次数，防止制动过热引起制动力热衰减；在冰雪、泥泞的路面上行驶，应用发动机牵阻制动可以防止侧滑。','','','','','','驾驶机动车下长坡时，利用惯性滑行可以减少燃油消耗，值得提倡。','','2017-05-02 11:01:04') [ RunTime:0.0020s ]

