SQL: INSERT INTO `jia_api` (`type`,`answer`,`chapter`,`explain`,`option1`,`option2`,`option3`,`option4`,`pic`,`question`,`type_`,`create_time`) VALUES ('C1','错','安全行车、文明驾驶基础知识','危险动作，下坡时，不准熄火或用空挡。如果下坡空档滑行，只靠刹车来控制车速，遇到连续下坡的时候可能会造成刹车负担过重，刹车片过热而失灵。','','','','','','车辆在下坡行驶时，可充分利用空挡滑行。','','2017-05-02 11:32:03') [ RunTime:0.0010s ]

